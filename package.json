{
  "name": "portfolio-app",
  "version": "1.0.0",
  "description": "Mobile-first portfolio web app",
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "dev:with-voiceflow": "VITE_VOICEFLOW_INTEGRATION=true vite",
    "dev:v0": "VITE_V0_INTEGRATION=true vite",
    "dev:full": "VITE_VOICEFLOW_INTEGRATION=true VITE_V0_INTEGRATION=true vite",
    "build": "next build",
    "start": "next start",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:integration": "vitest --run integration",
    "test:v0": "vitest --run v0",
    "v0:import": "node scripts/import-v0-component.js",
    "v0:test": "node scripts/test-v0-components.js",
    "commit:voiceflow": "git add references/exports-from-voiceflow/ src/data/voiceflowMapping.ts && git commit -m 'Update Voiceflow integration mapping'",
    "commit:component": "npm run test && git add src/components/ && git commit -m 'Add new React component with Voiceflow integration'",
    "commit:v0": "npm run test:v0 && git add src/components/v0/ && git commit -m 'Add v0.dev component with adventure integration'"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.82.0",
    "@vercel/blob": "^2.0.0",
    "canvas": "^2.11.2",
    "inngest": "^3.45.1",
    "lucide-react": "^0.554.0",
    "next": "^14.1.0",
    "pdfjs-dist": "^5.4.394",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.3.8",
    "react-pageflip": "^2.0.3",
    "sharp": "^0.34.5",
    "swiper": "^12.0.3"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/node": "^24.10.1",
    "@types/react": "18.3.27",
    "@types/react-dom": "^18.2.15",
    "@vitejs/plugin-react": "^4.1.1",
    "@vitest/coverage-v8": "^1.0.4",
    "@vitest/ui": "^1.0.4",
    "autoprefixer": "^10.4.22",
    "css-loader": "^7.1.2",
    "jsdom": "^23.0.1",
    "postcss": "^8.5.6",
    "postcss-loader": "^8.2.0",
    "style-loader": "^4.0.0",
    "tailwindcss": "3.4.1",
    "typescript": "5.9.3",
    "vite": "^5.0.0",
    "vitest": "^1.0.4"
  }
}
