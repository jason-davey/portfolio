{
  "name": "portfolio-app",
  "version": "1.0.0",
  "description": "Mobile-first portfolio web app",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:with-voiceflow": "VITE_VOICEFLOW_INTEGRATION=true vite",
    "dev:v0": "VITE_V0_INTEGRATION=true vite",
    "dev:full": "VITE_VOICEFLOW_INTEGRATION=true VITE_V0_INTEGRATION=true vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:integration": "vitest --run integration",
    "test:v0": "vitest --run v0",
    "v0:import": "node scripts/import-v0-component.js",
    "v0:test": "node scripts/test-v0-components.js",
    "commit:voiceflow": "git add references/exports-from-voiceflow/ src/data/voiceflowMapping.ts && git commit -m 'Update Voiceflow integration mapping'",
    "commit:component": "npm run test && git add src/components/ && git commit -m 'Add new React component with Voiceflow integration'",
    "commit:v0": "npm run test:v0 && git add src/components/v0/ && git commit -m 'Add v0.dev component with adventure integration'"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@vitejs/plugin-react": "^4.1.1",
    "jsdom": "^23.0.1",
    "typescript": "^5.2.2",
    "vite": "^5.0.0",
    "vitest": "^1.0.4",
    "@vitest/ui": "^1.0.4",
    "@vitest/coverage-v8": "^1.0.4"
  }
}